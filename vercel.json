{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/bebe.jpg",
      "dest": "/public/bebe.jpg"
    },
    {
      "src": "/(.*\\.(?:jpg|jpeg|png|gif|svg|css|js|html|ico))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}